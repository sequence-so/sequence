{
  "name": "sequence-api",
  "version": "0.1.1",
  "description": "API Server for Sequence",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --mode=production",
    "cron": "ts-node-dev --respawn -r tsconfig-paths/register ./scripts/cronDaemon.ts",
    "debug": "node -r ts-node/register -r tsconfig-paths/register --inspect-brk src/server.ts",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "exec": "node ./dist/scripts/check.js",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "migrate-and-start": "npm run migrate && npm run start",
    "migrate:generate": "npx sequelize-cli migration:generate --name",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --debug",
    "migrate": "npx sequelize-cli db:migrate --debug",
    "migration:generate": "yarn run migrate:generate",
    "script": "ts-node-dev --respawn -r tsconfig-paths/register ./scripts/track.ts",
    "serve": "node dist/bundle.js",
    "start": "npm run serve",
    "test:coverage": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc --reporter=html ts-mocha --exit --paths 'tests/**/*.spec.ts'",
    "test:debug": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-mocha --paths --inspect-brk 'tests/**/*.spec.ts'",
    "test:watch": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-mocha --paths --watch 'tests/**/*.ts' 'tests/**/*.spec.ts'",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ts-mocha --paths 'tests/**/*.spec.ts'",
    "watch-node": "ts-node-dev --respawn -r tsconfig-paths/register ./src/server.ts",
    "watch-ts": "tsc --build tsconfig.json -w"
  },
  "directories": {
    "src": "./src",
    "test": "./src"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/express-sslify": "^1.2.1",
